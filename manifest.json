{
    "name" : "Ukaton Side Mission Extension - A-Frame",
    "version" : "1.0",
    "manifest_version" : 2,

    "options_page": "options.html",

    "content_scripts" : [
        {
            "matches" : ["*://*/*"],
            "js" : ["script/content/script.js"],
            "css" : ["style/style.css"],
            "run_at" : "document_end"
        }
    ],
    "permissions" : ["activeTab", "tabs"],

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    
    "web_accessible_resources" : ["*"],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
       "128": "icon.png"
    }
}